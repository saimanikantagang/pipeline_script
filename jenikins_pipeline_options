pipeline{
    agent any
    options { 
        buildDiscarder(logRotator(numToKeepStr: '5')) 
        //disableConcurrentBuilds(abortPrevious: true)
        //disableConcurrentBuilds(abortPrevious: false)
        ansiColor('xterm')
    }
    stages{
        stage('Stage 1'){
            steps{
                echo "This is stage 1"
                sh 'sleep 10'
            }
        }
    }
}
