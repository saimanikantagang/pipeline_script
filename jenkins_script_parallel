pipeline{
    agent any
    stages{
        stage('stage 1'){
            steps{}
        }

        stage('parallel tests'){
            parallel{
                stage('stage 2-1'){
                    steps{
                        echo "stage 2-1"
                    }

                }
                stage('stage 2-2'){
                    steps{
                        echo "stage 2-1"
                    }
                }
            }
        }
    }
}